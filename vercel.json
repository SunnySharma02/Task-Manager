{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/tasks",
        "dest": "Task.js"
      },
      {
        "src": "/tasks/(.*)",
        "dest": "Task.js"
      },
      {
        "src": "/task/(.*)",
        "dest": "Task.js"
      },
      {
        "src": "/tasks/(.*)",
        "dest": "Task.js"
      },
      {
        "src": "/",
        "dest": "dist/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "dist/"
      }
    ]
  }